{
  "extends": "react-app",
  "parserOptions": {
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  },
  "globals": {
    "d3": true,
    "webvowl": true
  },
  "rules": {
    "no-unused-expressions": 1,
    "jsx-a11y/anchor-is-valid": 0,
    "array-callback-return": 0,
    "no-script-url": 0,
    "import/no-anonymous-default-export": 0
  }
}