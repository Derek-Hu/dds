    "antd": "^3.26.20",
    "antd-mobile": "^2.3.4",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.10.3",
    "decimal.js": "^10.2.1",
    "echarts": "^5.0.0",
    "ethers": "^5.0.31",
    "lodash": "^4.17.21",
    "node-xlsx": "^0.16.1",
    "numeral": "^2.0.6",
    "rc-banner-anim": "^2.4.5",
